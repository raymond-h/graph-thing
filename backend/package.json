{
    "name": "backend",
    "description": "Backend for Graph thing",
    "author": "Raymond Hammarling",
    "license": "MIT",
    "ava": {
        "require": [
            "babel-register"
        ],
        "babel": "inherit"
    },
    "scripts": {
        "lint": "eslint src/ test/",
        "test": "ava",
        "build": "babel -d lib/ src/",
        "watch:lint": "onchange src/ -- run-s lint",
        "watch:test": "ava -w",
        "watch:build": "babel -d lib/ src/ -w",
        "watch": "onchange src/ test/ -- run-s lint test build",
        "prepublish": "run-s lint test build",
        "start:dev": "nodemon --watch src --exec babel-node -- src/index.js"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0"
    },
    "devDependencies": {
        "ava": "^0.24.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.0.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "eslint": "^4.12.1",
        "nodemon": "^1.12.1",
        "npm-run-all": "^4.1.2",
        "onchange": "^3.2.1"
    }
}
